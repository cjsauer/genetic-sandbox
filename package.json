  "description": "A simulation of life itself",
  "main": "express.js",
  "scripts": {
    "build": "./node_modules/.bin/webpack",
    "start": "node express.js",
    "dev": "./node_modules/.bin/webpack-dev-server --inline --hot --content-base build/",
