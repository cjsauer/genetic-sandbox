  "dependencies": {
    "express": "^4.13.4"
  },
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-core": "^6.6.5",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-register": "^6.6.5",
    "chai": "^3.5.0",
    "clean-webpack-plugin": "^0.1.8",
    "coveralls": "^2.11.8",
    "css-loader": "^0.23.1",
    "file-loader": "^0.8.5",
    "html-loader": "^0.4.3",
