{
  "name": "genetic-sandbox",
  "version": "0.2.0",
  "description": "A simulation of life itself",
  "main": "express.js",
  "scripts": {
    "build": "webpack",
    "start": "node express.js",
    "postinstall": "npm run build",
    "dev": "webpack-dev-server --inline --hot --content-base build/",
    "test": "npm run lint -s && ./node_modules/.bin/babel-node ./node_modules/mocha/bin/_mocha --recursive",
    "test-watch": "./node_modules/.bin/babel-node ./node_modules/mocha/bin/_mocha --recursive --watch",
    "coverage": "./node_modules/.bin/babel-node ./node_modules/istanbul/lib/cli cover ./node_modules/mocha/bin/_mocha -- --recursive",
    "coveralls": "npm run coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "lint": "eslint \"**/*.js\"",
    "lint-fix": "eslint \"**/*.js\" --fix",
    "docs": "jsdoc2md --example-lang js --template ./site/_layouts/docs.html \"./src/modules/**/*.js\" --plugin dmd-kramdown-plugin > ./site/docs.md",
    "docs-deploy": "npm run docs && git subtree push --prefix site origin gh-pages",
    "stats": "jsdoc2md --stats \"./src/**/*.js\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cjsauer/genetic-sandbox.git"
  },
  "author": "Calvin Sauer",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cjsauer/genetic-sandbox/issues"
  },
  "homepage": "https://github.com/cjsauer/genetic-sandbox#readme",
  "dependencies": {
    "babel-cli": "^6.6.5",
    "babel-core": "^6.6.5",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-register": "^6.6.5",
    "clean-webpack-plugin": "^0.1.8",
    "css-loader": "^0.23.1",
    "express": "^4.13.4",
    "file-loader": "^0.8.5",
    "html-loader": "^0.4.3",
    "paper": "^0.9.25",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.14",
    "wolfy87-eventemitter": "^4.3.0"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "coveralls": "^2.11.8",
    "dmd-kramdown-plugin": "^0.1.0",
    "eslint": "^2.3.0",
    "eslint-config-standard": "^5.1.0",
    "eslint-plugin-promise": "^1.1.0",
    "eslint-plugin-standard": "^1.3.2",
    "istanbul": "^1.0.0-alpha",
    "jsdoc-to-markdown": "^1.3.3",
    "mocha": "^2.4.5",
    "mocha-lcov-reporter": "^1.2.0",
    "sinon": "^1.17.3",
    "webpack-dev-server": "^1.14.1"
  }
}
